# Gemini AI API 키
GEMINI_API_KEY=your_gemini_api_key_here

# 선택적 API 키들 (더 많은 데이터 소스를 위해)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
FINNHUB_API_KEY=your_finnhub_key_here

# 로깅 레벨
LOG_LEVEL=INFO

# 데이터 캐시 설정
CACHE_ENABLED=true
CACHE_TTL=3600

# 리포트 설정
REPORT_FORMAT=html,csv,pdf
OUTPUT_DIR=reports

# 테스트 모드
TEST_MODE=false