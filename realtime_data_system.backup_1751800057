    # ... (ë‚˜ë¨¸ì§€ ì½”ë“œëŠ” ë™ì¼)
    
    async def collect_market_data(self):
        """ì‹œì¥ ë°ì´í„° ìˆ˜ì§‘ ë£¨í”„"""
        while self.running:
            try:
                # ì‹œì¥ ë°ì´í„° ìˆ˜ì§‘
                market_data = self.kis_api.get_multiple_stocks(self.symbols)
