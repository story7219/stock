```python
"""
ğŸ§  ML/DL ë°ì´í„° ì •ì œ ëª¨ë“ˆ í…ŒìŠ¤íŠ¸
=====================================

ë°ì´í„° ì •ì œ ì—”ì§„ì˜ ê¸°ëŠ¥ì„ í…ŒìŠ¤íŠ¸í•˜ëŠ” ìŠ¤í¬ë¦½íŠ¸
- ëŒ€ìš©ëŸ‰ ë°ì´í„° ì²˜ë¦¬ ì„±ëŠ¥ í…ŒìŠ¤íŠ¸
- íŠ¹ì„± ì—”ì§€ë‹ˆì–´ë§ ê²€ì¦
- ë°ì´í„° í’ˆì§ˆ ë¶„ì„
- GPU ê°€ì† í…ŒìŠ¤íŠ¸
"""

import numpy as np
import pandas as pd
from datetime import datetime, timedelta
import time
import logging

# ë¡œê¹… ì„¤ì •
logging.basicConfig(level=logging.INFO,
                    format='%(asctime)s - %(levelname)s - %(message)s')
logger = logging.getLogger(__name__)


def generate_large_sample_data(n_samples: int = 100000) -> pd.DataFrame:
    """ëŒ€ìš©ëŸ‰ ìƒ˜í”Œ ë°ì´í„° ìƒì„±"""
    logger.info(f"ëŒ€ìš©ëŸ‰ ìƒ˜í”Œ ë°ì´í„° ìƒì„± ì¤‘: {n_samples:,}ê°œ ìƒ˜í”Œ")

    start_date = datetime(2020, 1, 1)
    dates = [start_date + timedelta(days=i)
             for i in range(n_samples)]

    base_price = 50000
    price_data = []
    current_price = base_price

    for i in range(n_samples):
        change = np.random.normal(0, 0.02)  # 2% í‘œì¤€í¸ì°¨
        current_price *= (1 + change)

        high_price = current_price * (1 + abs(np.random.normal(0, 0.01)))
        low_price = current_price * (1 - abs(np.random.normal(0, 0.01)))
        open_price = current_price * (1 + np.random.normal(0, 0.005))
        volume = int(np.random.normal(1000000, 200000))
        volume = max(volume, 100000)

        price_data.append({
            'date': dates[i],
            'stck_prpr': current_price,
            'stck_hgpr': high_price,
            'stck_lwpr': low_price,
            'stck_oprc': open_price,
            'acml_vol': volume,
            'acml_tr_pbmn': volume * current_price,
            'price_change': change,
            'symbol': '005930'
        })

    df = pd.DataFrame(price_data)
    logger.info(
        f"ìƒ˜í”Œ ë°ì´í„° ìƒì„± ì™„ë£Œ: {len(df):,}ê°œ ìƒ˜í”Œ, {len(df.columns)}ê°œ íŠ¹ì„±")
    return df


def test_data_processor():
    """ë°ì´í„° ì²˜ë¦¬ê¸° í…ŒìŠ¤íŠ¸ í•¨ìˆ˜"""
    try:
        df = generate_large_sample_data()
        # ... (ë‚˜ë¨¸ì§€ ë°ì´í„° ì²˜ë¦¬ ì½”ë“œ)
        logger.info("ë°ì´í„° ì²˜ë¦¬ ì™„ë£Œ")
    except Exception as e:
        logger.error(f"ë°ì´í„° ì²˜ë¦¬ ì¤‘ ì˜¤ë¥˜ ë°œìƒ: {e}")
```
